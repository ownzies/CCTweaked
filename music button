local basalt = require("basalt")

local main = basalt.createFrame()
local button = main 
        :addButton()
        :setPosition(4, 4)
        :setText("Happy Together")
        :onClick(
            function()
                shell.run("speaker play https://github.com/ownzies/CCTweaked/raw/main/happytogether%20(2).wav")
            end)

basalt.autoUpdate()
